{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: docling RAG",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "--url", "content/DocLayNet.pdf",
                "--collection", "ai_research_papers",
                "--model", "ibm-granite/granite-embedding-30m-english",
                "--query", "Can you provide the different labels recognized and the percentage of the total?"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "justMyCode": true
        },
        {
            "name": "Python: pymil",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pymil.py",
            "console": "integratedTerminal",
            "args": [
                "--list",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "justMyCode": true
        }
    ]
}
